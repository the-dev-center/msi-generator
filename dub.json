{
	"name": "msi-generator",
	"description": "Cross-platform MSI and MSIX package generator in pure D.",
	"authors": ["The Dev Center"],
	"license": "MIT",
	"targetType": "executable",
	"dependencies": {
		"cfb-d": {"path": "./libs/cfb-d"},
		"cab-d": {"path": "./libs/cab-d"},
		"msidb-d": {"path": "./libs/msidb-d"},
		"asdf": "~>0.7.17"
	},
	"configurations": [
		{
			"name": "executable",
			"targetType": "executable",
			"mainSourceFile": "source/app.d"
		},
		{
			"name": "library",
			"targetType": "library",
			"excludedSourceFiles": ["source/app.d"]
		},
		{
			"name": "prod",
			"targetType": "executable",
			"versions": ["UseStringUUIDs"],
			"mainSourceFile": "source/app.d"
		}
	]
}